import os
os.system("export DEBUG_PYOS=true")
os.getenv("DEBUG_PYOS")
print(os.getenv("DEBUG_PYOS"))
os.system("declare -x DEBUG_PYOS=true")
os.system("EXPORT  DEBUG_PYOS=true")
